<h2>Strava</h2>
<div class="row">
  <div class="col-sm-12">
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>ID</th><th>Type</th><th>Distance(m)</th><th>Duration(s)</th><th>Time</th><th>Source</th>
        </tr>
      </thead>
      <tbody id="merged-tbody">
        {{#each model as |activity|}}
        <tr>
          <td>{{activity.id}}</td>
          <td>{{activity.type}}</td>
          <td>{{activity.distance}}</td>
          <td>{{activity.duration}}</td>
          <td>{{activity.start_time}}</td>
          <td>{{activity.source}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
